<script>
    {# TODO Нужно вынести в отдельный компонет перезагрузка таблица fr table нужно убирать и делать обновленние данных#}


    let socket = io.connect('https://nbs-platforms.ru:8888');
    let user_id = `{{ user_id }}`;

    socket.on('reloadFrTable', (data) => {
        if (data.users_id == user_id) {
            if (document.getElementById('fr-count')) {
                $.ajax({
                    url: "../../orders/CountMyPriority",
                    method: "POST",
                    data: {
                        user_id: user_id,
                    }
                }).done((count) => {
                    document.getElementById('fr-count').textContent = count;
                });
            }
        }
    });


</script>


